  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.502.0",
    "firebase-admin": "^12.0.0",
    "json-bigint": "^1.0.0",
    "pg": "^8.12.0",
    "serverless-postgres": "^2.1.1",
    "telegraf": "^4.16.3",
    "typizator": "^3.2.1"
  },
  "devDependencies": {
    "@testcontainers/postgresql": "^10.6.0",
    "@types/jest": "^29.5.11",
    "@types/json-bigint": "^1.0.4",
    "@types/pg": "^8.11.6",
    "coverage-badges": "^1.0.7",
    "esbuild": "^0.20.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
